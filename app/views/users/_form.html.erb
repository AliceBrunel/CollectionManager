<%= form_with model: @user do |f| %>
  <%= label_tag(:username, "Name") %>
  <%= f.text_field :username %>
  <%= label_tag(:name, "Email") %>
  <%= f.text_field :email %>

  <%= f.collection_check_boxes :usertype_ids, Usertype.all, :id, :user_type %>

  <%= label_tag(:name, "Password") %>
  <%= f.text_field :password_digest %>
  <%= f.submit "Create" %>
<% end %>
